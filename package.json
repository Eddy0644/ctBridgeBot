{
  "name": "ctbridgebot",
  "version": "3.0.0",
  "description": "Bridge your messages from a notorious IM in China:{WeChat}, to an excellent IM:{Telegram}.",
  "superagent": "src/BotIndex.js",
  "scripts": {
    "build": "rm -f ct_temppacked.tar.gz && tar czvf ct_temppacked.tar.gz src modsrc config package.json",
    "p": "node src/BotIndex.js poll",
    "h1": "node src/BotIndex.js hook 1",
    "h2": "node src/BotIndex.js hook 2",
    "h0": "node src/BotIndex.js hook 0",
    "start": "node src/BotIndex.js poll",
    "puppet-install": "node node_modules/.bin/wechaty-puppet-install"
  },
  "keywords": [
    "wechaty",
    "telegram"
  ],
  "author": "netcyabc",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "dayjs": "^1.11.7",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.1.0",
    "file-box": "^1.5.5",
    "https-proxy-agent": "^6.1.0",
    "log4js": "^6.7.1",
    "md5": "^2.2.1",
    "node-machine-id": "^1.1.12",
    "node-schedule": "^1.3.2",
    "node-telegram-bot-api": "^0.61.0",
    "qrcode-terminal": "^0.12.0",
    "superagent": "^4.1.0",
    "tencentcloud-sdk-nodejs-asr": "^4.0.596",
    "wechaty": "^1.20.2",
    "wechaty-puppet-wechat": "^1.18.4",
    "xml2js": "^0.6.0"
  },
  "repository": "https://github.com/Eddy0644/ctBridgeBot"
}
